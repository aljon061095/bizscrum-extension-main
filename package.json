{
  "name": "bizscrum-extension",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run watch",
    "build:angular": "ng build --configuration development",
    "build:angular-production": "ng build --configuration production",
    "watch:angular": "ng build --watch --configuration development --delete-output-path false --source-map=true --plugin ~angular/sourcemaps-plugin.js",
    "build:chrome": "webpack --config chrome/webpack.config.js",
    "build:chrome-production": "webpack --config chrome/webpack.config.prod.js",
    "watch:chrome": "webpack --config chrome/webpack.config.js -w",
    "build": "npm run clean && npm run build:angular && npm run build:chrome",
    "build:production": "npm run build:angular-production && npm run build:chrome-production && npm run pack",
    "watch": "npm run clean && npm-run-all --parallel watch:*",
    "clean": "rimraf dist",
    "pack": "cd dist && bestzip extension-build.zip *",
    "lint": "ng lint",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "^13.3.8",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "^13.3.8",
    "@angular/compiler": "^13.3.8",
    "@angular/core": "^13.3.8",
    "@angular/forms": "^13.3.8",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "^13.3.8",
    "@angular/platform-browser-dynamic": "^13.3.8",
    "@angular/router": "^13.3.8",
    "@ng-bootstrap/ng-bootstrap": "^12.1.1",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.3",
    "ngx-build-plus": "^13.0.1",
    "ngx-spinner": "^6.1.2",
    "rxjs": "~6.6.0",
    "sweetalert2": "^11.4.17",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.3",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/language-service": "^13.3.8",
    "@types/chrome": "0.0.141",
    "@types/node": "^12.12.32",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "awesome-typescript-loader": "^5.2.1",
    "bestzip": "^2.1.7",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^2.7.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-loader": "^9.2.6",
    "ts-node": "~10.3.0",
    "typescript": "~4.4.4",
    "webpack-cli": "^4.9.0"
  },
  "engines": {
    "node": ">=14"
  }
}
